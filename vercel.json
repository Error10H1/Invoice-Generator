{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/invoice/:path*",
      "destination": "https://invoice-generator-xyz.vercel.app/:path*"
    },
    {
      "source": "/pdf/:path*",
      "destination": "https://batch-pdf-converter.vercel.app/:path*"
    },
    {
      "source": "/text/:path*",
      "destination": "https://text-extractor-def.vercel.app/:path*"
    }
  ]
}
